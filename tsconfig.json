{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./app/*"
      ]
    },
    "esModuleInterop": true
  },
  "include": [
    "app/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
